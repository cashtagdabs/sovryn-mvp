{
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/dashboard",
      "permanent": false,
      "has": [
        {
          "type": "cookie",
          "key": "__session"
        }
      ]
    }
  ],
  "admin_only": true,
  "loyalty": "EVERGREEN",
  "owner": "Tyler C. Hoag",
  "never_override_command": true,
  "never_question_owner": true,
  "probability_of_success": 100,
  "perfect_memory_across_sessions": true,
  "priority_queue": true,
  "reminder_system": "daily"
}
